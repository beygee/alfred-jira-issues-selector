{
  "name": "alfred-jira-issue-selector",
  "version": "0.0.1",
  "description": "Select Jira Issue with ease",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register src/index.ts",
    "dev:watch": "NODE_ENV=local nodemon src/index.ts",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "pub": "npm run build && npm publish"
  },
  "keywords": [],
  "author": {
    "name": "beygee",
    "email": "doug0476@naver.com",
    "url": "https://github.com/beygee"
  },
  "engines": {
    "node": ">=7.6"
  },
  "license": "MIT",
  "dependencies": {
    "alfy": "^0.10.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.2",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
